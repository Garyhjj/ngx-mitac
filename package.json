{
  "name": "ngx-mitac",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "npm run build && cross-env NODE_ENV=production node server.js",
    "serve": "npm run build && cross-env NODE_ENV=production pm2 start server.js --name=mitacEnd",
    "reServe": "npm run build && cross-env NODE_ENV=production pm2 reload mitacEnd",
    "preUpdate": "git checkout -- .",
    "update": "git pull && npm i && npm run build && cross-env NODE_ENV=production pm2 reload mitacEnd",
    "serve:dev": "npm run build:dev && cross-env NODE_ENV=development pm2 start server.js --name=mitacEnd",
    "reServe:dev": "npm run build:dev && cross-env NODE_ENV=development pm2 reload mitacEnd",
    "preUpdate:dev": "git checkout -- .",
    "update:dev": "git pull && npm i && npm run build:dev && cross-env NODE_ENV=development pm2 reload mitacEnd",
    "reload": "pm2 reload mitacEnd",
    "myCli": "node ./cli",
    "hmr": "ng serve --configuration hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "compression": "^1.7.1",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.2.0-rc.1",
    "jsqr": "^1.0.1",
    "moment": "^2.20.1",
    "ng-zorro-antd": "^1.0.0",
    "node-schedule": "^1.3.1",
    "rxjs": "^6.0.0",
    "signature_pad": "^3.0.0-beta.3",
    "xlsx": "^0.12.12",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@angular/service-worker": "^6.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/echarts": "^4.1.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/node-schedule": "^1.2.2",
    "body-parser": "^1.18.2",
    "cac": "^5.0.10",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.1.4",
    "express": "^4.16.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.6",
    "prettier": "^1.12.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^2.7.2"
  }
}
